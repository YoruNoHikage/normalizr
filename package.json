  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/normalizr.git"
  },
  "keywords": [
    "flux",
    "normalize",
    "api",
    "json"
  ],
  "files": [
    "dist",
    "lib",
    "src",
    "webpack.config.js",
    ".babelrc"
  ],
